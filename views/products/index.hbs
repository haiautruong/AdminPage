<div class="container">
            <div class="col-sm-12">
                <h2 class="text-uppercase"><span class="glyphicon glyphicon-list-alt"></span>Danh sách sản phẩm</h2>
                <div>
                    <form action="#" class="form-horizontal">
                        <div class="form-group">
                            <div class="col-sm-4">
                              <span class="input-group">
                                <input type="text" placeholder="Search for..." class="form-control">
                                <span class="input-group-btn">
                                    <button class="btn btn-primary"><span class="glyphicon glyphicon-search"></span></button>
                                </span>
                            </span>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="table-responsive">
                  <table class="table table-striped table-bordered">
                    <thead>
                      <tr class="success">
                        <th>Mã sản phẩm</th>
                        <th>Tên sản phẩm</th>
                        <th>Loại sản phẩm</th>
                        <th>Nhà sản xuất</th>
                        <th>Giá</th>
                        <th>Hành động</th>
                      </tr>
                    </thead>
                    <tbody>
                      {{#if}}
                      {{#each data}}
                      <tr>
                        <th scope="col">{{this.id}}</th>
                        <td>{{this.name}}</td>
                        <td>{{this.caterogyCode}}}</td>
                        <td>{{this.warranty}}</td>
                        <td>{{this.price}}</td>
                        <td>
                          <div class="btn-group">
                            <a href="#" class="btn btn-primary"><span class="glyphicon glyphicon-pencil"></span> Edit</a> 
                            <a href="#" class="btn btn-danger"><span class="glyphicon glyphicon-remove"></span> Delete</a>
                          </div>
                        </td>
                      </tr>
                      {{/each}}
                      {{/if}}
                    </tbody>
                  </table>
                </div>
            </div>
        </div>
    </div>
    